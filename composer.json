{
  "name": "madlab/evolve",
  "description": "Laravel A/B Testing Suite",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Ashley",
      "email": "nick@nickashley.dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "markrogoyski/math-php": "^2.11",
    "inertiajs/inertia-laravel": "^1.3|^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.5|^11.0"
  },
  "autoload": {
    "psr-4": {
      "MadLab\\Evolve\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MadLab\\Evolve\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MadLab\\Evolve\\EvolveServiceProvider"
      ]
    }
  }
}
