{
  "name": "madlab/evolve",
  "description": "Laravel A/B Testing Suite",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Ashley",
      "email": "nick@nickashley.dev"
    }
  ],
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "MadLab\\Evolve\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MadLab\\Evolve\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MadLab\\Evolve\\EvolveServiceProvider"
      ]
    }
  }
}
